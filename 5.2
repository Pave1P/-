def arithmetic_encode(symbols, probabilities, string):
    low = 0.0
    high = 1.0
    for char in string:
        range_ = high - low
        high = low + range_ * sum(probabilities[:symbols.index(char)+1])
        low = low + range_ * sum(probabilities[:symbols.index(char)])
    return (low + high) / 2

# Example usage
symbols = ['a', 'b', 'c', 'd', 'e', 'f']
probabilities = [0.10, 0.15, 0.05, 0.50, 0.10, 0.10]
encoded_value = arithmetic_encode(symbols, probabilities, "acefdb")
print("Encoded value:", encoded_value)
